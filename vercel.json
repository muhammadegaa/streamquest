{
  "version": 2,
  "builds": [
    { "src": "src/app.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/socket.io/(.*)", "dest": "src/app.js" },
    { "src": "/api/(.*)", "dest": "src/app.js" },
    { "src": "/auth/(.*)", "dest": "src/app.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "src/app.js" }
  ]
}
